    "axios": "^1.2.0",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "@types/axios": "^0.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0"
  }
